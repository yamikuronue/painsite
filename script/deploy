#!/bin/bash

cat <<END >  ~/.ssh/known_hosts
# www.janebaileybooks.com SSH-2.0-OpenSSH_6.0p1 Debian-4+deb7u2
|1|R8qCausXeSTAB4QGnZBG4YcWmq0=|j++hfA0Kl7D7ZKOtL1XFtbG7rMc= ssh-rsa 
END

scp -i id_rsa -r _site www.janebaileybooks.com:/home/travis/
ssh -i id_rsa www.janebaileybooks.com rm -rf /var/www/Sites/painsite/site
ssh -i id_rsa www.janebaileybooks.com mv /home/travis/_site /var/www/Sites/painsite/site
